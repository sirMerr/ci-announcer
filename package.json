{
  "name": "ci-announcer",
  "version": "1.0.0",
  "description": "Probot app which reports failing builds on PRs with error logs",
  "author": "Tiffany Le-Nguyen <letiffany.nguyen@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/sirMerr/ci-announcer.git",
  "homepage": "https://github.com/sirMerr/ci-announcer",
  "bugs": "https://github.com/sirMerr/ci-announcer/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "precommit": "pretty-quick --staged",
    "start:watch": "nodemon --exec \"npm start\" --watch src --watch index.ts --ignore build",
    "start": "npm build && probot run ./build/index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.4",
    "dotenv": "^6.0.0",
    "probot": "^7.0.0-typescript.4",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.6.0",
    "smee-client": "^1.0.1",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
